  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "services-api": {
        "kind": "odata",
        "credentials": {
          "destination": "teste-destination",
          "path": "/api/v1/reports",
          "forwardAuthToken": true
        }
      },
      "middleware-api": {
        "kind": "odata",
        "credentials": {
          "destination": "middleware-api-destination",
          "path": "/sap/opu/odata/sprolims/SRV_LABSERV_SRV",
          "forwardAuthToken": true
        }
      },
      "db": {
        "impl": "@cap-js/postgres",
        "dialect": "postgres",
        "kind": "postgres",
        "[sqlite]": {
          "kind": "sql"
        },
        "vcap": {
          "label": "user-provided"
        },
        "pool": {
          "acquireTimeoutMillis": 10000
        }
      },
      "[hybrid]": {
        "auth": "xsuaa"
      }
    },
    "hana": {
      "deploy-format": "hdbtable",
      "syntax": "hdi"
    },
    "build": {
      "target": "gen",
      "tasks": [
        {
          "for": "hana",
          "src": "srv/index.cds",
          "options": {
            "contentCdsrcJson": false,
            "model": [
              "srv/index.cds"
            ]
          }
        },
        {
          "for": "nodejs",
          "src": "srv/index.cds",
          "options": {
            "contentCdsrcJson": false,
            "model": [
              "srv/index.cds"
            ]
          }
        }
      ]
    }
  },
